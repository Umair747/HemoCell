<?xml version="1.0" ?>

<cell>
	<shellDensity> 1. </shellDensity>
	<k_stretch> 1.0e-3 </k_stretch>
	<k_shear> 1.0e-3 </k_shear>
	<k_bend> 1.0e-3 </k_bend>
	<k_volume> 1.0e-3 </k_volume>
	<k_surface> 1.0e-3 </k_surface>
</cell>


<parameters>
	<Re> 0.1 </Re>   <!--Reynolds number-->
	<rho_p> 1025 </rho_p>   <!--Density, Physical units [kg/m^3]-->
	<nu_p> 1.7e-6 </nu_p>   <!-- Dynamic viscosity, physical units [m^2/s]-->
	<tau> 1.0 </tau>
	<dx> 1e-6 </dx>
	<nx> 50 </nx>
	<ny> 10 </ny>
	<nz> 10 </nz>
</parameters>


<ibm>
	<forceToFluid> 0 </forceToFluid>
	<shape> 1 </shape> <!-- shape: Sphere:0, RBC:1 -->
	<radius> 1.0 </radius> <!-- Radius of the particle in Lattice Units (dx) -->
</ibm>

<sim>
	<tmax> 10000000000 </tmax> <!-- total number of iterations -->
	<tmeas> 100 </tmeas> <!-- interval after which data is written -->> 
	<npar> 6 </npar> <!-- Number of particles > -->
</sim>



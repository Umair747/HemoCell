<?xml version="1.0" ?>
<ficsion>
<caseId> caseId </caseId>
<cell>
    <rbcModel> 0 </rbcModel> <!--ShapeMemory: [0], MeanValues: [1] -->
    <shellDensity> 1.0 </shellDensity>

    <kBend> 200e0 </kBend> <!--bending modulus (in k_BT units, 4.142e-21 N m) [75, 25-171, 200, 69, 1000, 4000] {KruegerThesis2011, FedosovCaswell2010b}-->
    <kVolume> 6000.0 </kVolume> <!--volume conservation coefficient (dimensionless) [600, 6000] {FedosovCaswell2010b, Pivkin2008, LiJ2005}--> 
    <kSurface> 5900.0 </kSurface> <!--surface conservation coefficient (dimensionless) [600, 5900]     {FedosovCaswell2010b, Pivkin2008, LiJ2005}--> 
<!-- 0.5e0 -->
    <kShear> 100.0e0 </kShear> <!--local area constrain coefficient (dimensionless) [0, 10, 100]--> 
    <etaM> 0.0e-6 </etaM> <!--membrane viscosity (in N s/m (Pa s m) units ) [0.022e-6, 0.022e-7] {FedosovFedosovCaswell2010}-->
<!-- +++++ -->
<!-- Testing phase-->
<!-- +++++ -->
    <kWLC> 0.25 </kWLC> <!--Worm-like chain modulus coefficient (dimensionless) [1.0] -->
    <kRep> 1.0e0 </kRep> <!--Repulsive coefficient for WLC modulus (dimensionless) [1.0] -->
    <kElastic> 0.0e0 </kElastic> <!--WLC elastic modulus coefficient, q=1 (dimensionless) [0.0] -->
    <kRest> 0.0e-4 </kRest> <!--Resting modulus (in N/m) [0.0, 1.0e-4]-->
    <kStretch> 0.0e-10</kStretch> <!--spring modulus (in N) [0.0]-->
</cell>

<parameters>
    <flowType> 4 </flowType>   <!--flowType: Poisseuille:[0], Couette:[1], Cell Stretching [3]-->
    <Re> 0.1 </Re>   <!--Reynolds number-->
    <shearRate> 0 </shearRate>   <!--Shear Rate for Couette flow, Physical units [1/s]. Using ny-->
    <stretchForce> 100.0 </stretchForce>   <!--Stretch Force using the X direction (in pN). [100]-->
    <deflationRatio> 1.0 </deflationRatio> <!-- Ratio of Final to Initial ratio [0.65] -->
    <relaxationTime> 1000 </relaxationTime> <!-- Relaxation time for deflation and stretching force [5000] -->
</parameters>


<ibm>
    <forceToFluid> 1 </forceToFluid>
    <ibmKernel> 4 </ibmKernel> <!-- ibmKernel: Phi2 [2], Phi3 [3], Phi4 [4],  Phi4c [5] -->
    <shape> 1 </shape> <!-- shape: Sphere:[0], RBC:[1], Cell(defined):[2] -->
    <cellPath>lib/RBC.stl</cellPath>
    <radius> 4.3e-6 </radius> <!-- Radius of the particle in [m] (dx) [4.284 for RBC, 3.3 for sphere] -->
    <minNumOfTriangles> 200 </minNumOfTriangles> <!--Minimun numbers of triangles per cell. Not always exact.-->
</ibm>

<domain>
    <rhoP> 1025 </rhoP>   <!--Density of the surrounding fluid, Physical units [kg/m^3]-->
    <nuP> 1.7e-6 </nuP>   <!-- Dynamic viscosity of the surrounding fluid, physical units [m^2/s]-->
    <tau> 1.0 </tau>  <!--Relaxation time of the Lattice Boltzmann scheme-->
    <dx> 2e-6 </dx> <!--Physical length of 1 Lattice Unit -->
    <nx> 20 </nx> <!--Size of domain in Lattice Units -->
    <ny> 10 </ny>
    <nz> 20 </nz>
</domain>

<sim>
    <tmax> 50000000 </tmax> <!-- total number of iterations -->
    <tmeas> 1000 </tmeas> <!-- interval after which data is written -->> 
    <npar> 1 </npar> <!-- Number of particles > -->
</sim>
</ficsion>

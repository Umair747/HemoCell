<?xml version="1.0" ?>
<hemocell>
<MaterialModel>
    <comment>Parameters for the HO RBC constitutive model.</comment>
    <name>RBC</name>
    <eta_m> 5e-10 </eta_m> <!-- Membrane viscosity. [Ns/m]-->
    <eta_v> 0.002 </eta_v> <!-- Volume viscosity of cytoplasm, acting between outer vertices around angle edges [Pa s] -->
    <kBend> 50.0 </kBend> <!-- Bending force modulus for membrane + cytoskeleton ( in k_BT units, 4.142e-21 N m) [80] -->
    <kLink> 15.0 </kLink> <!-- Link force coefficient (dimensionless) [15.0] -->
    <link_eq> 1 </link_eq> <!-- Link lenght in micrometers -->
    <bend_eq> 0.5 </bend_eq> <!-- bending angle target in radials? -->
</MaterialModel>
<MaxVertices> 10 </MaxVertices> <!-- Maximum number of vertices per vWF -->
</hemocell>

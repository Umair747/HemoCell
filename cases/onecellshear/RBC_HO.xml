<?xml version="1.0" ?>
<hemocell>
<MaterialModel>
    <comment>Parameters for the HO RBC constitutive model.</comment>
    <name>RBC</name>
    <eta_m>0.0015</eta_m> <!-- Membrane viscosity acting along the edges [Pa s] -->
    <eta_b>0.001</eta_b> <!-- Bending viscosity (for cytoskeleton) acting between outer vertices around angle edges [Pa s] -->
    <kBend> 35.0 </kBend> <!-- Bending force modulus (in k_BT units, 4.142e-21 N m) -->
    <kVolume> 100.0 </kVolume> <!-- Volume conservation coefficient (dimensionless) [600, 6000] --> 
    <kArea> 80.0 </kArea> <!--Local area conservation coefficient (dimensionless) [0, 10, 100] --> 
    <!-- NOTE: kBend should != kArea. The larger the difference, the more stable the model. -->
    <kLink> 4.35 </kLink> <!-- Link force coefficient (dimensionless) [1.0] -->
    <minNumTriangles> 600 </minNumTriangles> <!--Minimun numbers of triangles per cell. Not always exact.-->
    <radius> 3.91e-6 </radius> <!-- Radius of the RBC in [um] -->
</MaterialModel>
</hemocell>
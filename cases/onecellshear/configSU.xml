<?xml version="1.0" ?>
<hemocell>
<caseId> caseId </caseId>
<cellModel>
    <rbcModel> 0 </rbcModel> <!--ShapeMemory: [0], MeanValues: [1], Intermediate Model [2] -->
    <shellDensity> 1.0 </shellDensity>

    <kBend> 100e0 </kBend> <!--bending modulus (in k_BT units, 4.142e-21 N m) [75, 25-171, 200, 69, 1000, 4000] {KruegerThesis2011, FedosovCaswell2010b}-->
    <kVolume> 6000.0 </kVolume> <!--volume conservation coefficient (dimensionless) [600, 6000] {FedosovCaswell2010b, Pivkin2008, LiJ2005}--> 
    <kSurface> 5900.0 </kSurface> <!--surface conservation coefficient (dimensionless) [600, 5900]     {FedosovCaswell2010b, Pivkin2008, LiJ2005}--> 
<!-- 0.5e0 -->
    <kShear> 100.0e0 </kShear> <!--local area constrain coefficient (dimensionless) [0, 10, 100]--> 
    <etaM> 0.0e-6 </etaM> <!-- NOT WORKING ATM --> <!--membrane viscosity (in N s/m (Pa s m) units ) [0.022e-6, 0.022e-7] {FedosovFedosovCaswell2010}-->
    <kWLC> 0.42 </kWLC> <!--Worm-like chain modulus coefficient (dimensionless) [1.0] -->
    <eqLengthRatio> 2.6 </eqLengthRatio> <!--Ratio of equilibrium and maximum length used in WLC chain (dimensionless) [3.17 Pivkin2008, 2.2 FedosovFedosovCaswell2010] -->
    <kRep> 1.0e0 </kRep> <!--Repulsive coefficient for WLC modulus (dimensionless) [1.0] -->
    <kElastic> 0.0e0 </kElastic> <!--WLC elastic modulus coefficient, q=1 (dimensionless) [0.0] -->
    <kRest> 0.0e-4 </kRest> <!--Resting modulus (in N/m) [0.0, 1.0e-4]-->
    <kStretch> 0.0e-10</kStretch> <!--spring modulus (in N) [0.0]-->
    <persistenceLengthFine> 7.5e-9 </persistenceLengthFine>
</cellModel>

<parameters>
    <shearrate> 111.0 </shearrate>   <!--Shear rate for the fluid domain. [s^-1] [25]. -->
    <warmup> 500 </warmup> <!-- Number of LBM iterations to prepare fluid field. -->
    <eulerAngles> 0 90 0 </eulerAngles> <!-- In degrees -->
</parameters>


<ibm>
    <radius> 3.91e-6 </radius> <!-- Radius of the particle in [m] (dx) [3.3e-6, 3.91e-6, XX and 4.284 for shapes [0,1,2,3] respectively -->
    <minNumOfTriangles> 600 </minNumOfTriangles> <!--Minimun numbers of triangles per cell. Not always exact.-->
</ibm>

<domain>
    <rhoP> 1025 </rhoP>   <!--Density of the surrounding fluid, Physical units [kg/m^3]-->
    <nuP> 0.7e-6 </nuP>   <!-- Dynamic viscosity of the surrounding fluid, physical units [m^2/s]-->
    <dx> 1.00e-6 </dx> <!--Physical length of 1 Lattice Unit -->
    <dt> 2.0e-7 </dt> <!-- Time step for the LBM system. A negative value will set Tau=1 and calc. the corresponding time-step. -->
    <timeStepSize> 1 </timeStepSize> <!-- Update particle material model after how many fluid time steps. [Integer] -->
</domain>

<sim>
    <tmax> 100000 </tmax> <!-- total number of iterations -->
    <tmeas> 5000 </tmeas> <!-- interval after which data is written --> 
</sim>

</hemocell>

<?xml version="1.0" ?>
<ficsion>
<caseId> caseId </caseId>
<cellModel>
    <rbcModel> 0 </rbcModel> <!-- ShapeMemory: [0], MeanValues: [1], Intermediate Model [2] -->
    <materialModel> 2 </materialModel> <!-- Original Suresh: [0], Sures w. new bending: [1], Higher order model: [2] -->
    <shellDensity> 1.0 </shellDensity>
    <kBend> 225 </kBend> <!-- bending modulus (in k_BT units, 4.142e-21 N m) [75, 25-171, 200, 69, 1000, 4000] {KruegerThesis2011, FedosovCaswell2010b}-->
    <kVolume> 800.0 </kVolume> <!-- volume conservation coefficient (dimensionless) [600, 6000] {FedosovCaswell2010b, Pivkin2008, LiJ2005}-->
    <kSurface> 0.0 </kSurface> <!-- global surface conservation coefficient (dimensionless) [600, 5900]     {FedosovCaswell2010b, Pivkin2008, LiJ2005}-->
<!-- 0.5e0 -->
    <kShear> 100.0e0 </kShear> <!-- local area constrain coefficient (dimensionless) [0, 10, 100]-->
    <etaM> 0.0e-6 </etaM> <!-- NOT WORKING ATM --> <!--membrane viscosity (in N s/m (Pa s m) units ) [0.022e-6, 0.022e-7] {FedosovFedosovCaswell2010}-->
    <kWLC> 0.5 </kWLC> <!--Worm-like chain modulus coefficient (dimensionless) [1.0] -->
    <eqLengthRatio> 2.2 </eqLengthRatio> <!--Ratio of equilibrium and maximum length used in WLC chain (dimensionless) [3.17 Pivkin2008, 2.2 FedosovFedosovCaswell2010] -->
    <kRep> 1.0e0 </kRep> <!--Repulsive coefficient for WLC modulus (dimensionless) [1.0] -->
    <kElastic> 0.0e0 </kElastic> <!--WLC elastic modulus coefficient, q=1 (dimensionless) [0.0] -->
    <kRest> 0.0e-4 </kRest> <!--Resting modulus (in N/m) [0.0, 1.0e-4]-->
    <kStretch> 0.0e-10</kStretch> <!--spring modulus (in N) [0.0]-->
</cellModel>

<parameters>
    <stretchForce> 37.899 </stretchForce>   <!--Stretch Force using the X direction (in pN). [100]. -->
</parameters>

<ibm>
    <ibmKernel> 2 </ibmKernel> <!-- ibmKernel: Phi2 [2], Phi3 [3], Phi4 [4],  Phi4c [5] -->
    <ibmScheme> 0 </ibmScheme> <!-- ibmScheme: Euler [0], Adams-Bashforth [1] -->
    <radius> 3.91e-6 </radius> <!-- Radius of the particle in [m] (dx) [3.3e-6, 3.91e-6, XX and 4.284 for shapes [0,1,2,3] respectively -->
    <minNumOfTriangles> 600 </minNumOfTriangles> <!--Minimun numbers of triangles per cell. Not always exact.-->
</ibm>

<domain>
    <rhoP> 1025 </rhoP>   <!--Density of the surrounding fluid, Physical units [kg/m^3]-->
    <nuP> 1.7e-6 </nuP>   <!-- Dynamic viscosity of the surrounding fluid, physical units [m^2/s]-->
    <tau> 1.0 </tau>  <!--Relaxation time of the Lattice Boltzmann scheme-->
    <dx> 1.00e-6 </dx> <!--Physical length of 1 Lattice Unit -->
    <dt> 5e-7 </dt>
    <timeStepSize> 1 </timeStepSize> <!-- Update particle material model after how many fluid time steps. [Integer] -->
</domain>

<sim>
    <tmax> 250000 </tmax> <!-- total number of iterations -->
    <tmeas> 200 </tmeas> <!-- interval after which data is written --> 
</sim>

</ficsion>
